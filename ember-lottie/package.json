{
  "name": "@qonto/ember-lottie",
  "version": "1.0.0",
  "description": "Render lottie after effects animations in Ember.js",
  "keywords": [
    "ember-addon",
    "after effects",
    "animation",
    "lottie"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:qonto/ember-lottie.git"
  },
  "license": "MIT",
  "author": "",
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist"
  ],
  "scripts": {
    "build": "concurrently 'npm:build:*'",
    "build:js": "rollup --config",
    "build:types": "glint --declaration",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "concurrently 'npm:start:*'",
    "start:js": "rollup --config --watch --no-watch.clearScreen",
    "start:types": "glint -d --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "prepack": "rollup --config"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.2",
    "@embroider/addon-shim": "^1.8.7",
    "@glimmer/component": "^1.1.2",
    "ember-window-mock": "^0.9.0",
    "lottie-web": "^5.12.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/plugin-proposal-async-generator-functions": "7.20.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.23.3",
    "@babel/plugin-proposal-json-strings": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-optional-catch-binding": "7.18.6",
    "@babel/preset-typescript": "7.23.3",
    "@babel/runtime": "^7.23.4",
    "@embroider/addon-dev": "^4.1.2",
    "@glint/core": "1.2.1",
    "@glint/environment-ember-loose": "1.2.1",
    "@qonto/eslint-config-typescript": "1.0.0-rc.0",
    "@rollup/plugin-babel": "6.0.4",
    "@tsconfig/ember": "3.0.2",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.7.5",
    "concurrently": "^8.2.2",
    "ember-modifier": "^4.1.0",
    "ember-template-lint": "^5.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "rollup": "4.5.1",
    "rollup-plugin-copy": "3.5.0",
    "typescript": "5.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/lottie.js": "./dist/_app_/components/lottie.js"
    }
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0 || ^5.0.0"
  }
}
